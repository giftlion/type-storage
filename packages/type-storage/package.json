{
  "name": "@giftlion/typed-storage",
  "version": "0.0.4",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src --format cjs,esm --dts",
    "test": "vitest"
  },
  "dependencies": {
    "react": "19.1.0",
    "tsup": "^8.5.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.19.21",
    "@types/react": "^19",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giftlion/type-storage"
  },
  "homepage": "https://github.com/giftlion/type-storage#readme",
  "bugs": {
    "url": "https://github.com/giftlion/type-storage/issues"
  },
  "publishConfig": {
    "access": "public"
  }
